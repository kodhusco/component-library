{
  "name": "kodhus-design-library",
  "version": "0.21.44",
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "axios": "^0.19.0",
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "styled-components": "^4.4.0"
  },
  "scripts": {
    "test": "react-scripts test",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build-storybook-docs": "build-storybook -s public --docs",
    "build": "BABEL_ENV=production babel src -d dist; babel src/style.css --out-dir dist --copy-files; babel src/fonts --out-dir dist/fonts --copy-files",
    "prepare": "npm run build",
    "release": "auto shipit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@storybook/addon-a11y": "^5.2.2",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-docs": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addon-storysource": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "auto": "^7.8.0",
    "prettier": "^1.18.2"
  },
  "description": "Kodhus ui library",
  "main": "dist/index.js",
  "repository": "https://github.com/kodhusco/component-library.git",
  "author": "kodhus <kodhus@kodhus.com>",
  "license": "MIT",
  "homepage": "https://github.com/kodhusco/component-library.git#readme",
  "private": false
}
